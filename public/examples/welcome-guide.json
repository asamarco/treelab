    {
      "id": "mk05e56t-qsxk0rgjr",
      "name": "Hierarchical Nodes",
      "templateId": "FeatureTemplate",
      "data": {
        "2025-08-19T08:57:55.257Z0m2c3ze": "Hierarchical Nodes",
        "2025-08-19T08:57:55.257Z1ddpobd": "Organize your information in a nested tree structure. You can add child nodes, create siblings, move them around, delete them.",
        "2025-08-24T06:58:51.175Z0.27552086254079367": "Normal"
      },
